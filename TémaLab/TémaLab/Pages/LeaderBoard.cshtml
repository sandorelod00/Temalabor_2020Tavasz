    <link rel="stylesheet" type="text/css" href="/css/Base.css">
    <link rel="stylesheet" type="text/css" href="/css/site.css">
    <link rel="stylesheet" type="text/css" href="~/css/LeaderBoard.css" />

</head>

<body>
    <div class="container">
        <div class="row">
            <h1 class="LeaderBoardTitle">LeaderBoard</h1>
        </div>

        <div class="row">
            <table class="table table-dark table-striped table-hover table-responsive-md LeaderBoardTabel">
                <thead>
                    <tr>
                        <th scope="col">Place</th>
                        <th scope="col">User Name</th>
                        <th scope="col">Score</th>
                        <th scope="col">Last Competition</th>
                        <th scope="col">Rank</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>1.</td>
                        <td> <a class="TableLink" asp-area="" asp-page="/OtherProfile">OtherUserName</a></td>
                        <td>19872 points</td>
                        <td>WorldChampionship</td>
                        <td>Plat</td>
                    </tr>
                    <tr>
                        <td>2.</td>
                        <td> <a class="TableLink" asp-area="" asp-page="/OtherProfile">OtherUserName</a></td>
                        <td>12872 points</td>
                        <td>WorldChampionship</td>
                        <td>Diamond</td>
                    </tr>
                    <tr>
                        <td>3.</td>
                        <td> <a class="TableLink" asp-area="" asp-page="/OtherProfile">OtherUserName</a></td>
                        <td>9872 points</td>
                        <td>WorldChampionship</td>
                        <td>Diamond</td>
                    </tr>
                    <tr>
                        <td>4.</td>
                        <td> <a class="TableLink" asp-area="" asp-page="/OtherProfile">OtherUserName</a></td>
                        <td>7478 points</td>
                        <td>WorldChampionship</td>
                        <td>Gold</td>
                    </tr>
                </tbody>
            </table>
        </div>

    </div>


    



</body>





</html>